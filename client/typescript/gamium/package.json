{
  "name": "gamium",
  "version": "2.0.6",
  "description": "Gamium is an SDK that allows you to automate gameplay",
  "license": "MIT",
  "author": "dogu",
  "homepage": "https://github.com/dogu-team/gamium.git",
  "keywords": [
    "game",
    "qa",
    "gametest",
    "gametesting",
    "gametestautomation",
    "unity",
    "unreal",
    "test",
    "testautomation",
    "android",
    "ios",
    "windows",
    "macos"
  ],
  "type": "commonjs",
  "exports": {
    ".": {
      "require": {
        "types": "./build/cjs/index.d.ts",
        "default": "./build/cjs/index.cjs"
      },
      "import": {
        "types": "./build/esm/index.d.ts",
        "default": "./build/esm/index.mjs"
      }
    },
    "./common": {
      "require": {
        "types": "./build/cjs/common/index.d.ts",
        "default": "./build/cjs/common/index.cjs"
      },
      "import": {
        "types": "./build/esm/common/index.d.ts",
        "default": "./build/esm/common/index.mjs"
      }
    }
  },
  "main": "./build/cjs/index.cjs",
  "files": [
    "build/**/*.{cjs,mjs,d.ts}"
  ],
  "scripts": {
    "build": "ts-node scripts/build.ts",
    "clean": "shx rm -rf build",
    "dev": "nodemon",
    "rebuild": "yarn run clean && yarn run build",
    "watch": "clear; echo gamium Watching... && nodemon",
    "publish": "yarn run rebuild && npm publish"
  },
  "types": "./build/cjs/index.d.ts",
  "dependencies": {
    "flatbuffers": "22.10.26",
    "gl-matrix": "3.4.3",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@types/assert": "1.5.6",
    "@types/node": "18.15.11",
    "@types/shelljs": "0.8.11",
    "@types/ws": "8",
    "ansi-styles": "6.2.1",
    "cross-env": "7.0.3",
    "glob": "10.2.3",
    "mjscjs": "1.1.0",
    "nodemon": "2.0.22",
    "shelljs": "0.8.5",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
